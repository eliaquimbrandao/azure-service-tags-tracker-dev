{
  "version": 2,
  "builds": [
    {
      "src": "api/subscribe.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/unsubscribe.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/request_unsubscribe.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/plan_status.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/upgrade.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/subscribe",
      "dest": "/api/subscribe.py"
    },
    {
      "src": "/api/unsubscribe",
      "dest": "/api/unsubscribe.py"
    },
    {
      "src": "/api/request-unsubscribe",
      "dest": "/api/request_unsubscribe.py"
    },
    {
      "src": "/api/plan-status",
      "dest": "/api/plan_status.py"
    },
    {
      "src": "/api/upgrade",
      "dest": "/api/upgrade.py"
    }
  ]
}
